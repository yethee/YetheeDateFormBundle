<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="yethee.form.date_type_extension" class="Yethee\DateFormBundle\Form\Extension\DateTypeExtension">
            <tag name="form.type_extension" alias="date" />
        </service>

        <service id="yethee.form.datetime_type_extension" class="Yethee\DateFormBundle\Form\Extension\DateTimeTypeExtension">
            <tag name="form.type_extension" alias="datetime" />
        </service>
    </services>
</container>
